    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.13.2",
    "browserify": "^13.1.0",
    "chai": "^3.5.0",
    "eslint": "^3.2.2",
    "express": "^4.14.0",
    "mocha": "^3.0.2",
